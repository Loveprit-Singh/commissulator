<header>
  <%= current_avatar.name %>
</header>

<%= form_with :model => @avatar do |avatar_form| %>
  <%= avatar_form.label :first_name %>
  <%= avatar_form.text_field :first_name %><br>
  <%= avatar_form.label :last_name %>
  <%= avatar_form.text_field :last_name %><br>
  <%= avatar_form.label :phone_number %>
  <%= avatar_form.text_field :phone_number %><br>
  <%= avatar_form.label :google_email %>
  <%= avatar_form.text_field :google_email %><br>
  <%= avatar_form.submit :Save %>
<% end %>
