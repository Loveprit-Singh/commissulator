<% @agents.each do |agent| %>
  <%= div_for agent, :class => :agent_list do %>
    <%= link_to_name_with_icon agent %>
    <%= deals_by agent %>
    <%= weekly_deals_by agent %>
    <%= commissions_for agent %>
    <%= assistants_of agent %>
    <%= agent_registration_path agent %>
  <% end %>
<% end %>
