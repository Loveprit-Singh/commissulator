<header>
  <%= deals_by current_agent %>
</header>

<%= form_with :model => @agent do |agent_form| %>
  <%= agent_form.label :first_name %>
  <%= agent_form.text_field :first_name %><br>
  <%= agent_form.label :last_name %>
  <%= agent_form.text_field :last_name %><br>
  <%= agent_form.label :phone_number %>
  <%= agent_form.text_field :phone_number %><br>
  <%= agent_form.submit :Save %>
<% end %>
