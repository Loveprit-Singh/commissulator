<p>
  <strong>First name:</strong>
  <%= @client.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @client.last_name %>
</p>

<p>
  <strong>Date of birth:</strong>
  <%= @client.date_of_birth %>
</p>

<% @client.employments.each do |employment| %>
  <%= div_for employment do %>
    <%= employment.name %>
    <%= link_to_name employment.employer %>
  <% end %>
<% end %>

<%= link_to fa_icon(:highlighter), edit_client_path(@client), :title => 'Edit' %>
